{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.ender_dragon_mount": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"setup",
						"jaw_movement",
						"neck_head_movement",
						"tail_movement"
					],
					"transitions": [
						{
							"wings_limbs_movement": "!query.is_on_ground"
						}
					]
				},
				"wings_limbs_movement": {
					"animations": [
						"wings_limbs_movement"
					],
					"transitions": [
						{
							"default": "query.ground_speed == 0.0 && query.is_on_ground"
						}
					]
				}
			}
		}
	}
}