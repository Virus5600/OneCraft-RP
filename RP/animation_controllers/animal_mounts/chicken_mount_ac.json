{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.chicken_mount": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"move": "query.ground_speed > 0.0 && query.is_on_ground"
						},
						{
							"fall": "!query.is_on_ground"
						}
					]
				},
				"move": {
					"animations": [
						"move"
					],
					"transitions": [
						{
							"default": "query.ground_speed == 0.0 && query.is_on_ground"
						},
						{
							"fall": "!query.is_on_ground"
						}
					]
				},
				"fall": {
					"animations": [
						"fall"
					],
					"transitions": [
						{
							"default": "query.ground_speed == 0.0 && query.is_on_ground"
						},
						{
							"move": "query.ground_speed > 0.0 && query.is_on_ground"
						}
					]
				}
			}
		}
	}
}