{
    "format_version": "1.16.100",
    "minecraft:block": {
        "description": {
            "identifier": "ocrp:new_enchanted_table"
        },
        "components": {
            "minecraft:creative_category": {
                "group": "ItemGroup.name.Construction",
                "category": "Construction"
            },
            "minecraft:material_instances": {
                "*": {
                    "texture": "new_enchanted_table",
                    "render_method": "alpha_test"
                }
            },
            "minecraft:on_placed": {
                "event": "rotation"
            },
            "minecraft:queued_ticking": {
                "looping": true,
                "interval_range": [
                    10,
                    10
                ],
                "on_tick": {
                    "event": "ocrp:enchanted_particle",
                    "target": "self"
                }
            },
            "minecraft:pick_collision": {
                "origin": [
                    -8,
                    0,
                    -8
                ],
                "size": [
                    16,
                    12,
                    16
                ]
            },
            "minecraft:on_interact": {
                "event": "ocrp:tag_interact"
            },
            "minecraft:entity_collision": {
                "origin": [
                    -8,
                    0,
                    -8
                ],
                "size": [
                    16,
                    12,
                    16
                ]
            },
            "minecraft:on_player_destroyed": {
                "event": "ocrp:despawn_entity"
            },
            "minecraft:geometry": "geometry.new_enchanted_table",
            "minecraft:destroy_time": 1,
            "minecraft:block_light_absorption": 0,
            "minecraft:block_light_emission": 1
        },
        "events": {
            "ocrp:despawn_entity": {
                "run_command": {
                    "command": [
                        "event entity @e[type=ocrp:book_enchanted,r=1 ] ocrp:entity_despawn"
                    ]
                }
            },
            "ocrp:enchanted_particle": {
                "run_command": {
                    "command": [
                        "particle ocrp:enchanted_particle ~~~"
                    ]
                }
            },
            "ocrp:tag_interact": {
                "run_command": {
                    "command": [
                        "tag @s add interact"
                    ],
                    "target": "player"
                }
            },
            "rotation": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "summon ocrp:book_enchanted ~~0.31~"
                            ]
                        }
                    }
                ]
            }
        }
    }
}