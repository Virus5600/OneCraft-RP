{
	"namespace": "hud",
  
	"rns_data": {
		"$update_string1": "rns_data_update",
		"$update_string2": "fnr_data_update",
		"$update_string3": "hook_data_update",
		"type": "panel",
		"size": [ 0, 0 ],
		"controls": [
			{
				"roll_stamina_data_control": {
					"type": "panel",
					"size": [ 0, 0 ],
					"bindings": [
						{
							"binding_name": "#hud_title_text_string"
						},
						{
							"binding_name": "#show_survival_ui"
						},
						{
							"binding_name": "#hud_title_text_string",
							"binding_name_override": "#stored_text",
							"binding_condition": "visibility_changed"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#stored_text - 'a')",
							"target_property_name": "#data"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 10000)",
							"target_property_name": "#stamina"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 1000) - (#stamina * 10)",
							"target_property_name": "#stamina_style"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 10) - (#stamina * 1000) - (#stamina_style * 100)",
							"target_property_name": "#roll"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 1) - (#stamina * 10000) - (#stamina_style * 1000) - (#roll * 10)",
							"target_property_name": "#roll_style"
						},
						{
							"binding_type": "view",
							"source_property_name": "(not (#hud_title_text_string = #stored_text) and not ((#hud_title_text_string - $update_string1) = #hud_title_text_string))",
							"target_property_name": "#visible"
						}
					]
				}
			},
			{
				"fatigue_climb_data_control": {
					"type": "panel",
					"size": [ 0, 0 ],
					"bindings": [
						{
							"binding_name": "#hud_title_text_string"
						},
						{
							"binding_name": "#show_survival_ui"
						},
						{
							"binding_name": "#hud_title_text_string",
							"binding_name_override": "#stored_text",
							"binding_condition": "visibility_changed"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#stored_text - 'a')",
							"target_property_name": "#data"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 100000)",
							"target_property_name": "#fatigue"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 10000) - (#fatigue * 10)",
							"target_property_name": "#fatigue_style"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 100) - (#fatigue * 1000) - (#fatigue_style * 100)",
							"target_property_name": "#climb"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 10) - (#fatigue * 10000) - (#fatigue_style * 1000) - (#climb * 10)",
							"target_property_name": "#climb_style"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 1) - (#fatigue * 100000) - (#fatigue_style * 10000) - (#climb * 100) - (#climb_style * 10)",
							"target_property_name": "#hook"
						},
						{
							"binding_type": "view",
							"source_property_name": "((#fatigue * 5) / 16)",
							"target_property_name": "#fatigue_1"
						},
						{
							"binding_type": "view",
							"source_property_name": "(not (#hud_title_text_string = #stored_text) and not ((#hud_title_text_string - $update_string2) = #hud_title_text_string))",
							"target_property_name": "#visible"
						}
					]
				}
			},
			{
				"hook_data_control": {
					"type": "panel",
					"size": [ 0, 0 ],
					"bindings": [
						{
							"binding_name": "#hud_title_text_string"
						},
						{
							"binding_name": "#show_survival_ui"
						},
						{
							"binding_name": "#hud_title_text_string",
							"binding_name_override": "#stored_text",
							"binding_condition": "visibility_changed"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#stored_text - 'a')",
							"target_property_name": "#data"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 1000)",
							"target_property_name": "#hook_charge"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 100) - (#hook_charge * 10)",
							"target_property_name": "#charge_capacity"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 10) - (#hook_charge * 100) - (#charge_capacity * 10)",
							"target_property_name": "#hook_style"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#data / 1) - (#hook_charge * 1000) - (#charge_capacity * 100) - (#hook_style * 10)",
							"target_property_name": "#hook_display"
						},
						{
							"binding_type": "view",
							"source_property_name": "(not (#hud_title_text_string = #stored_text) and not ((#hud_title_text_string - $update_string3) = #hud_title_text_string))",
							"target_property_name": "#visible"
						}
					]
				}
			}
		]
	},

	"grappling_hook_crosshair_icon": {
		"type": "image",
		"texture": "textures/ui/grappling_hook/crosshair_normal",
		"size": [
			"32px",
			"16px"
		]
	},

	"image_sequence": {
		"type": "image",
		"size": "$size",
		"uv": "$uv",
		"uv_size": "$uv_size",
		"alpha": "$alpha",
		"bindings": [
			{
				"binding_type": "view",
				"source_control_name": "$container",
				"source_property_name": "($path + $target)",
				"target_property_name": "#texture"
			}
		]
	},

	"image_fill": {
		"type": "image",
		"size": "$size",
		"uv": "$uv",
		"uv_size": "$uv_size",
		"alpha": "$alpha",
		"texture": "$path",
		"clip_direction": "$clip_direction",
		"tiled": "$tiled",
		"bindings": [
			{
				"binding_type": "view",
				"source_control_name": "$container",
				"source_property_name": "(1.0 - ($target / $fill_ratio))",
				"target_property_name": "#clip_ratio"
			}
		]
	},

	"background_tiled": {
		"type": "image",
		"size": "$size",
		"uv": "$uv",
		"uv_size": "$uv_size",
		"texture": "$background",
		"alpha": "$alpha",
		"tiled": true
	},

	"background_image": {
		"type": "image",
		"size": "$size",
		"uv": "$uv",
		"uv_size": "$uv_size",
		"texture": "$background",
		"alpha": "$alpha"
	},
	
	"custom_render": {
		"type": "panel",
		"$tiled|default": false,
		"$clip_direction|default": "up",
		"$fill_ratio|default": 15.0,
		"$size|default": [ 17, 17 ],
		"$uv_size|default": "$size",
		"$uv|default": [ 0, 0 ],
		"$target|default": "#roll",
		"$container|default": "roll_stamina_data_control",
		"$alpha|default": 1,
		"$background|default": "textures/ui/roll/default/empty",
		"$path|default": "textures/ui/roll/default/cooldown_",
		"$model|default": "hud.image_sequence",
		"$background_model|default": "hud.background_image",
		"size": "$size",
		"anchor_from": "center",
		"anchor_to": "center",
		"controls": [
			{
				"background@$background_model": {}
			},
			{
				"image@$model": {}
			}
		]
    },

